[configuration]

entry_symbol = "utility_ai_library_init"
compatibility_minimum = "4.5"
reloadable = true

[icons]

Need = "icons/Need.svg"
Agent = "icons/Agent.svg"
Action = "icons/Action.svg"

[libraries]

macos.debug = "bin/libutilityai.macos.template_debug.framework"
macos.release = "bin/libutilityai.macos.template_release.framework"
ios.debug = "bin/libutilityai.ios.template_debug.xcframework"
ios.release = "bin/libutilityai.ios.template_release.xcframework"
windows.debug.x86_32 = "bin/libutilityai.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "bin/libutilityai.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "bin/libutilityai.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "bin/libutilityai.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "bin/libutilityai.linux.template_debug.x86_64.so"
linux.release.x86_64 = "bin/libutilityai.linux.template_release.x86_64.so"
linux.debug.arm64 = "bin/libutilityai.linux.template_debug.arm64.so"
linux.release.arm64 = "bin/libutilityai.linux.template_release.arm64.so"
linux.debug.rv64 = "bin/libutilityai.linux.template_debug.rv64.so"
linux.release.rv64 = "bin/libutilityai.linux.template_release.rv64.so"
android.debug.x86_64 = "bin/libutilityai.android.template_debug.x86_64.so"
android.release.x86_64 = "bin/libutilityai.android.template_release.x86_64.so"
android.debug.arm64 = "bin/libutilityai.android.template_debug.arm64.so"
android.release.arm64 = "bin/libutilityai.android.template_release.arm64.so"

[dependencies]
ios.debug = {
    "bin/libgodot-cpp.ios.template_debug.xcframework": ""
}
ios.release = {
    "bin/libgodot-cpp.ios.template_release.xcframework": ""
}

